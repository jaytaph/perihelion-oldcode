<!-- Inventions -->

  [BLOCK buildings]
    <table class=standard align=center>
       <tr><th colspan=5>Invented Buildings</th><tr>
       [BLOCK buildingrow]
         <tr>
           [BLOCK buildingcell]
             <td><a href="{building_href}"><img border="0" align="center" src="{building_img}" width="100" height="100"></a></td>
           [END buildingcell]
         </tr>           
       [END buildingrow]
       <tr><th colspan=5>Discoveries {building_discovery_percentage}% completed</th></tr>
    </table>
  [END buildings]
  <br>
  <br>

  [BLOCK vessels]
    <table class=standard align=center>
       <tr><th colspan=5>Invented Vessels</th><tr>
       [BLOCK vesselrow]
         <tr>
           [BLOCK vesselcell]  
             <td><a href="{vessel_href}"><img border="0" align="center" src="{vessel_img}" width="100" height="100"></a></td>
           [END vesselcell]
         </tr>
       [END vesselrow]
       <tr><th colspan=5>Discoveries {vessel_discovery_percentage}% completed</th></tr>
    </table>
  [END vessels]
  <br>
  <br>

  [BLOCK inventions]
    <table class=standard align=center>
       <tr><th colspan=5>Invented Inventions</th><tr>
       [BLOCK inventionrow]
         <tr>
           [BLOCK inventioncell]  
             <td><a href="{invention_href}"><img border="0" align="center" src="{invention_img}" width="100" height="100"></a></td>
           [END inventioncell]
         </tr>
       [END inventionrow]
       <tr><th colspan=5>Discoveries {vessel_discovery_percentage}% completed</th></tr>
    </table>
  [END inventions]
  <br>
  <br>

<!-- End Inventions -->
